{
    "name": "gufrondev/aspi-snap-bi",
    "description": "SNAP-BI Multi Providers",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "role": "Developer",
        "name": "Gufron Rahmatain",
        "email": "baponk.smoker@gmail.com",
        "homepage": "https://github.com/gufrondev"
      }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^10.0"
    },
    "autoload": {
      "psr-4": {
        "GufronDev\\AspiSnapBI\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "GufronDev\\AspiSnapBI\\Tests\\": "tests/"
      }
    },
    "extra": {
        "laravel": {
            "providers": "GufronDev\\AspiSnapBI\\AspiSnapBIServiceProvider"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true,
      "preferred-install": "dist",
      "optimize-autoloader": true
    }
}
